<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="216.0" prefWidth="314.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="subComponents.mergeDialog.MergeDialogController">
   <Label layoutX="21.0" layoutY="24.0" text="Merging branches" />
   <Label layoutX="21.0" layoutY="57.0" text="You are currently on branch" />
   <Button fx:id="okButton" defaultButton="true" layoutX="167.0" layoutY="165.0" mnemonicParsing="false" onAction="#okButtonAction" text="OK" />
   <Button fx:id="cancelButton" cancelButton="true" layoutX="110.0" layoutY="165.0" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel" />
   <Label fx:id="headBranchNameLabel" layoutX="174.0" layoutY="57.0" text="HEAD">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
   </Label>
   <Label layoutX="21.0" layoutY="74.0" text="Please choose which branch to merge into current:" />
   <ChoiceBox fx:id="branchesChoiceBox" layoutX="80.0" layoutY="117.0" prefWidth="150.0" />
</AnchorPane>
